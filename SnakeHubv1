-- Scripts:

local function JZMJIN_fake_script() -- SnakeHub.DraggableLocalScript 
	local script = Instance.new('LocalScript', SnakeHub)

	--By DriedUpCat
	
	frame = script.Parent.MainHub
	frame.Draggable = true
	frame.Active = true
	frame.Selectable = true
end
coroutine.wrap(JZMJIN_fake_script)()
local function NQNFCIZ_fake_script() -- CloseButton.LocalScript 
	local script = Instance.new('LocalScript', CloseButton)

	--Close Button:
	local frame = script.Parent.Parent
	
	script.Parent.MouseButton1Click:Connect(function()
		frame.Visible = false
	end)
end
coroutine.wrap(NQNFCIZ_fake_script)()
local function QMOFYN_fake_script() -- OpenButtonMain.LocalScript 
	local script = Instance.new('LocalScript', OpenButtonMain)

	--Open Button:
	local frame = script.Parent.Parent.MainHub --Change "Frame" to your frame.
	
	script.Parent.MouseButton1Click:Connect(function()
		frame.Visible = true
	end)
end
coroutine.wrap(QMOFYN_fake_script)()
local function QMME_fake_script() -- CloseButtonLogin.LocalScript 
	local script = Instance.new('LocalScript', CloseButtonLogin)

	--Close Button:
	local frame = script.Parent.Parent
	
	script.Parent.MouseButton1Click:Connect(function()
		frame.Visible = false
	end)
end
coroutine.wrap(QMME_fake_script)()
